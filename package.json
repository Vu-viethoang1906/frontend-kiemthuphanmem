{
  "name": "duanfontreact",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/generative-ai": "^0.24.1",
    "@react-keycloak/core": "^3.2.0",
    "@react-keycloak/web": "^3.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-toastify": "^4.0.2",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "cors": "^2.8.5",
    "dependencies": "^0.0.1",
    "framer-motion": "^12.23.18",
    "googleapis": "^166.0.0",
    "groq-sdk": "^0.34.0",
    "keycloak-js": "^26.2.0",
    "lucide-react": "^0.546.0",
    "openai": "^6.6.0",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "test:fe": "npm run test:ci",
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "cross-env GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:prod:lowmem": "cross-env CI=true NODE_OPTIONS=--max_old_space_size=1024 DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test --watchAll=false --coverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md,html}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "e2e": "cypress open",
    "e2e:ci": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./reports/junit",
    "outputName": "junit-fe.xml",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º "
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/src/test/__mocks__/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "/src/pages/Landing/",
      "/src/pages/Introduction/",
      "/src/pages/Settings/",
      "/src/pages/Filters/",
      "/src/pages/Group/",
      "/src/pages/Project/",
      "/src/pages/User/",
      "/src/pages/Board/",
      "/src/pages/Admin/PermissionManagement.tsx",
      "/src/components/SplashScreen.tsx",
      "/src/components/NotificationBell.tsx",
      "/src/components/Sidebar.tsx",
      "/src/config/",
      "/src/keycloack/",
      "/src/socket.ts",
      "/src/reportWebVitals.ts"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "cobertura",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 20
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "cypress": "^15.7.0",
    "husky": "^9.1.7",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "msw": "^1.3.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.1",
    "tailwindcss": "^3.4.4"
  }
}
